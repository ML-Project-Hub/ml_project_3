   "source": [
    "def softmax(vector):\n",
    "    z_exp = [numpy.exp(i) for i in vector]\n",
    "    sum_z_exp = sum(z_exp)\n",
    "    softmax = [round(i / sum_z_exp, 3) for i in z_exp]\n",
    "    return softmax"
   ]
